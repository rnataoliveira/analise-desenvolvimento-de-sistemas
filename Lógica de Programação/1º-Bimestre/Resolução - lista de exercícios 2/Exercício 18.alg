algoritmo "Exercício 18 - Lista de exercício 2"

var
   idade:inteiro
inicio
      //Entrada
      escreval("Este programa mostra se há maioridade ou não.")
      escreval("Digite a idade: ")
      leia(idade)
      
      //Processamento
      se (idade >= 18) entao
         escreval("VOCÊ É DE MAIOR. ")
      senao
         escreval("VOCÊ É DE MENOR. ")
      fimse
      
fimalgoritmo
