algoritmo "Exercício 10 - Lista de exercício 1"

var
   lado,area:real
inicio
      // Entrada
      escreval("Este programa calcula a área de um quadrado.")
      escreval("Entre com a medida: ")
      leia(lado)
      
      //Processamento

      area <- lado * lado


      //Saída
      escreval("A área do quadrado é: ",area:3:2)


fimalgoritmo
